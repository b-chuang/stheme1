{% include 'bold-product' with product, hide_action: 'break' %}{% if bold_hidden_product %}{% break %}{%endif %}

<script type="text/javascript">
  
  jQuery( document ).ready(function(){
    
    jQuery('input[type=checkbox]').on('click', function(){
      jQuery(this).parent().parent().toggleClass("checked");
    });
    
    
  	jQuery('#productIntro').html(jQuery('#priceDesc').html());
    jQuery('#productIntromob').html(jQuery('#priceDesc').html());
    jQuery('#priceDesc').html(''); 

    jQuery('.accessories-item a').contents().unwrap();
    
    jQuery('.accessories-item').click(function () {
      jQuery(this).find('input[type=checkbox]').prop("checked", !jQuery(this).find('input[type=checkbox]').prop("checked"));
    });
    setTimeout(function(){
      jQuery('.accessories-item a').contents().unwrap();
      jQuery('.accessories-item, .accessory-price, .accessories-img-wrapper, .accessories-right, .accessories-title').click(function () {
      jQuery(this).find('input[type=checkbox]').prop("checked", !jQuery(this).find('input[type=checkbox]').prop("checked"));
    });
    },1000);
    
  }) 
 
</script>
